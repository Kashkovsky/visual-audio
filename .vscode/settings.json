{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.configPath": ".prettierrc",
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.exclude": {
    "**packages/engine/lib": true,
    "**node_modules": true,
    "**.parcel-cache": true,
    "**packages/creator/dist": true,
    "**packages/creator/node_modules": true,
    "**packages/engine/tsconfig.tsbuildinfo": true,
    "**packages/engine/node_modules": true,
    "**packages/visuals/lib": true,
    "**packages/visuals/node_modules": true
  }
}
