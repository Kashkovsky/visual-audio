{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.configPath": ".prettierrc",
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.exclude": {
    "**packages/engine/lib": false,
    "**node_modules": false,
    "**.parcel-cache": false,
    "**packages/creator/dist": false,
    "**packages/creator/node_modules": false,
    "**packages/engine/tsconfig.tsbuildinfo": false,
    "**packages/engine/node_modules": false,
    "**packages/visuals/lib": false,
    "**packages/visuals/node_modules": false
  }
}
