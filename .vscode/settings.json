{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.configPath": ".prettierrc",
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.exclude": {
    "**packages/*/lib": false,
    "**node_modules": false,
    "**.parcel-cache": false,
    "**packages/*/node_modules": false,
    "**packages/*/tsconfig.tsbuildinfo": false,
    "**dist": false,
    "**packages/*/*.build.log": false,
    "**packages/*/dist": false,
    "**common/temp": false
  }
}
